        <i class="fas fa-bars cursor-pointer" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" role="button"></i>

        <div class="dropdown-menu bg-darky" aria-labelledby="dropdownMenuButton">
